{% extends 'AdminConsoleBundle::page.html.twig' %}
{% form_theme form 'bootstrap_3_layout.html.twig' %}
{% block pageContent %}
<div class="table-viewer-header clearfix">
<span class="table-viewer-topbar-title"><span class="subHeader"></span></span>
</div>
<div class="table-content">
	{{ form_start(form) }}	
		<div class="pageFormContent">
			{% block formContent %} {% endblock %}
		</div>
		<button type="submit" id="btnSubmit" class="btn btn-primary">提交</button>
		<button type="button" id="btnReset" class="btn btn-default">重置</button>
		</div>
	{{ form_end(form) }}	
</div>
{% endblock %}